	</siteModel>

	<!--                                                                         -->
	<statistic id="mu" name="mu">
		<siteModel idref="siteModel"/>
	</statistic>
	

	<!-- Likelihood for tree given sequence data                                 -->
	<treeDataLikelihood id="treeLikelihood" useAmbiguities="false" usePreOrder="true">
		<partition>
			<patterns idref="patterns"/>
			<siteModel idref="siteModel"/>
		</partition>
		<treeModel idref="treeModel"/>
		<branchRates idref="branchRates"/>
	</treeDataLikelihood>
	

	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="power">
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="kappa"/>
		</scaleOperator>
		<deltaExchange delta="0.01" weight="1">
			<parameter idref="frequencies"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="branchRates.scale"/>
		</scaleOperator>
		<hamiltonianMonteCarloOperator weight="1" nSteps="4" stepSize="0.01" mode="vanilla" gradientCheckCount="0" preconditioning="diagonal" preconditioningUpdateFrequency="10">
			<jointGradient>
				<hessian>
					<distributionLikelihood idref="ratesPrior"/>
					<parameter idref="branchRates.rates"/>
				</hessian>
				<branchRateGradient traitName="Sequence">
					<treeDataLikelihood idref="treeLikelihood"/>
				</branchRateGradient>
			</jointGradient>
			<parameter idref="branchRates.rates"/>
			<signTransform>
				<parameter idref="branchRates.rates"/>
			</signTransform>
		</hamiltonianMonteCarloOperator>
		<subtreeLeap size="1.0" weight="71">
			<treeModel idref="treeModel"/>
		</subtreeLeap>
		<fixedHeightSubtreePruneRegraft weight="7.1">
			<treeModel idref="treeModel"/>
		</fixedHeightSubtreePruneRegraft>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="exponential.popSize"/>
		</scaleOperator>
		<randomWalkOperator windowSize="1.0" weight="3">
			<parameter idref="exponential.growthRate"/>
		</randomWalkOperator>
	</operators>
	

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="300000000" autoOptimize="true">
		<joint id="joint">
			<prior id="prior">
				<booleanLikelihood>
					<monophylyStatistic idref="monophyly(DeltaPlus)"/>
					<monophylyStatistic idref="monophyly(Beta)"/>
					<monophylyStatistic idref="monophyly(Gamma)"/>
					<monophylyStatistic idref="monophyly(EG5)"/>
					<monophylyStatistic idref="monophyly(XBB)"/>
					<monophylyStatistic idref="monophyly(Alpha)"/>
					<monophylyStatistic idref="monophyly(XBB.1.16)"/>
					<monophylyStatistic idref="monophyly(Delta)"/>
					<monophylyStatistic idref="monophyly(BA5)"/>
					<monophylyStatistic idref="monophyly(XBB.1.5)"/>
					<monophylyStatistic idref="monophyly(BA2)"/>
					<monophylyStatistic idref="monophyly(Mu)"/>
					<monophylyStatistic idref="monophyly(BA1)"/>
					<monophylyStatistic idref="monophyly(Lambda)"/>
				</booleanLikelihood>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="kappa"/>
				</logNormalPrior>
				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="frequencies"/>
				</dirichletPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="alpha"/>
				</exponentialPrior>
				<gammaPrior shape="0.001" scale="1000.0" offset="0.0">
					<parameter idref="exponential.popSize"/>
				</gammaPrior>
				<laplacePrior mean="0.0" scale="100.0">
					<parameter idref="exponential.growthRate"/>
				</laplacePrior>
				<coalescentLikelihood idref="coalescent"/>
				
				
				<branchRates idref="branchRates"/>
			</prior>
			<likelihood id="likelihood">
				<treeDataLikelihood idref="treeLikelihood"/>
			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="rootHeight" sf="6" width="12">
				<treeHeightStatistic idref="rootHeight"/>
			</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="sars_cov_2_cpass.log" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<treeHeightStatistic idref="rootHeight"/>
			<treeLengthStatistic idref="treeLength"/>
			<!-- <tmrcaStatistic idref="tmrca(Alpha)"/>
			<tmrcaStatistic idref="tmrca(Beta)"/>
			<tmrcaStatistic idref="tmrca(Gamma)"/>
			<tmrcaStatistic idref="tmrca(Lambda)"/>
			<tmrcaStatistic idref="tmrca(XBB)"/>
			<tmrcaStatistic idref="tmrca(XBB.1.5)"/>
			<tmrcaStatistic idref="tmrca(XBB.1.16)"/>
			<tmrcaStatistic idref="tmrca(Delta)"/>
			<tmrcaStatistic idref="tmrca(DeltaPlus)"/>
			<tmrcaStatistic idref="tmrca(BA1)"/>
			<tmrcaStatistic idref="tmrca(BA2)"/>
			<tmrcaStatistic idref="tmrca(BA5)"/>
			<tmrcaStatistic idref="tmrca(EG5)"/>
			<tmrcaStatistic idref="tmrca(SARS-CoV-1)"/>
			<tmrcaStatistic idref="tmrca(Mu)"/> -->
			<parameter idref="exponential.popSize"/>
			<parameter idref="exponential.growthRate"/>
			<parameter idref="kappa"/>
			<parameter idref="frequencies"/>
			<parameter idref="alpha"/>
			<parameter idref="branchRates.rate"/>
			<parameter idref="branchRates.scale"/>
			<rateStatistic idref="meanRate"/>
			<rateStatistic idref="coefficientOfVariation"/>
			<rateCovarianceStatistic idref="covariance"/>
			<treeDataLikelihood idref="treeLikelihood"/>
			<branchRates idref="branchRates"/>
			<coalescentLikelihood idref="coalescent"/>
			
		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="1000" nexusFormat="true" fileName="sars_cov_2_cpass.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="rate">
				<arbitraryBranchRates idref="branchRates"/>
			</trait>
			<joint idref="joint"/>
		</logTree>

		<!-- write state of Markov chain to checkpoint file                          -->
		<logCheckpoint id="checkpointFileLog" checkpointEvery="1000000" checkpointFinal="10000000" fileName="sars_cov_2_cpass.chkpt" overwrite="false"/>
	</mcmc>
	
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
	
</beast>
